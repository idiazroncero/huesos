{
  "name": "huesos",
  "version": "0.0.7",
  "description": "Huesos is idiaz.roncero's front-end starterkit. It provides a basic SCSS-JS osamenta by combining some preferred and battle-tested tools.",
  "main": "index.js",
  "scripts": {
    "build": "yarn run styleguide; webpack --config webpack.config.js",
    "watch": "yarn run styleguide; webpack --watch --config webpack.config.js",
    "huesos": "yarn run build",
    "styleguide": "cd src/kss-src; npm run sass",
    "lint:js": "eslint src/js webpack.*.js --cache --debug",
    "lint:css": "stylelint dist/css/*.css"
  },
  "author": "Nacho Diaz",
  "license": "MIT",
  "repository": "git@github.com:idiazroncero/huesos.git",
  "devDependencies": {
    "animate-scss": "^3.2.1",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.5.2",
    "bootstrap": "^4.0.0-alpha.6",
    "breakpoint-sass": "^2.7.1",
    "clean-webpack-plugin": "^0.1.16",
    "concise.css": "^4.1.2",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "eslint-loader": "^1.8.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "family.scss": "^1.0.8",
    "foundation-sites": "^6.4.1",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.5.0",
    "html-webpack-plugin": "^2.29.0",
    "kss": "^3.0.0-beta.18",
    "kss-webpack-plugin": "^1.2.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "ritmo": "^0.5.0",
    "sanitize.css": "^5.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "susy": "^2.2.12",
    "uikit": "^3.0.0-beta.25",
    "waypoints": "^4.0.1",
    "webpack": "^3.0.0"
  },
  "browserslist": [
    "> 1%"
  ],
  "dependencies": {
    "file-loader": "^1.1.6"
  }
}
